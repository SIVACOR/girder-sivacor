[paths]
source =
    girder_sivacor/
    */site-packages/girder_sivacor

[run]
branch = true
parallel = true
source = girder_sivacor
omit = 
    */tests/*
    */test_*
    */.tox/*
    */venv/*
    */env/*
    */build/*
    */dist/*

[report]
show_missing = true
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[xml]
output = coverage.xml

[html]
directory = htmlcov
